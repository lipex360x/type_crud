{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "semi":"off",
    "import/no-unresolved":"off",
    "import/extensions":"off",
    "no-console":"off",
    "class-methods-use-this":"off",
    "no-unused-vars":"off"
  }
}
